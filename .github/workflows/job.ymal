name: 'GitHub Actions README.md update songs'

on:
  push:
    branches: 
      - main
  schedule:
    - cron: '30 * * * *'

jobs:
  readme-update-song:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout
        uses: actions/checkout@v2
      - name: 'Set up Python'
        uses: actions/setup-python@v1
        with:
            python-version: 3.7
      - name: 'Install lib'
        run: pip install requests
      - name: 'work'
        run: python ./sohyunQVQ/163music.py